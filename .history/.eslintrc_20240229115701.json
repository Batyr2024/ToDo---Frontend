{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-plusplus": "off",
        "max-len": [
            "error",
            {
                "code": 150
            }
        ],
        "operator-linebreak": "off",
        "no-alert": "off"
    }
}